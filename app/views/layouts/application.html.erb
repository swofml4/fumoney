<!DOCTYPE html>
<html>
<head>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => false %>
  <%= csrf_meta_tags %>

  
	</head>

	
</head>
<body>
	<div class="container">
		<div class="header">
			
			<div class="header-img">
				<%= image_tag("Money.png") %>
			</div>
			<h1 class="header-heading">FU Money</h1>
		</div>
		<div class="nav-bar">
			<ul class="nav">
				<li><%= link_to 'New Sim', simulations_build_simulation_path %></li>
                <li><%= link_to 'View Results', simulations_results_path %></li>
                <li><%= link_to 'Manage Sims', simulations_manage_path %></li>
	            <% if user_signed_in? %>
	                <% if current_user.admin_flag %>
		                <li><%= link_to 'Asset Types', asset_types_path %></li>
		                <li><%= link_to 'Corr Collection', correlation_collections_path %></li>
	                <% end %>
	               <% end %>
                <li><%= link_to 'External Sources', simulations_sources_path %></li>
                <li><%= link_to 'Matrix Test', simulations_matrix_test_path %></li>
                <% if user_signed_in? %>
                	<li><%= link_to 'Log Out', destroy_user_session_path, :method => :delete %></li>
                <% else %>
                	<li><%= link_to 'Sign Up', new_user_registration_path %></li>
                	<li><%= link_to 'Log In', new_user_session_path %></li>
                <% end %>
			</ul>
		</div>
		<div class="content">
			<div class="main">
 	 			<p id="notice"><%= notice %></p>
 	 			<p class="alert"><%= alert %></p>

<%= yield %>

            </div>
        </div>
        <div class="footer">
	    	&copy; 2016 Michael Swofford
	    </div>
    </div>

           

</body>
</html>
